<div class="bg0 m-t-23 p-b-140" style="margin-top: 80px;">
    <div class="container py-5 mx-y m-r-40 m-l-40 p-b-140  ">

        <form action="" method="post">

            <!-- profile end -->
            <div class="row">
                <div class="col-md-6 mb-3">
                   <label class="font-weight-bold" for="name"> Name * </label>
                    <input type="text" class="form-control " id="name" name="name" value="" required>
                </div><!-- End .col-md-6 -->

                <div class="col-md-6 mb-3">
                   <label class="font-weight-bold" for="email">Email *</label>
                    <input type="text" class="form-control " id="email" name="email" value="" required>
                </div><!-- End .col-md-6 -->
            </div><!-- End .row -->

            <div class="mb-3 w-50">
                <label class="font-weight-bold" for="mobile">Mobile *</label>
                <input type="tel" class="form-control " id="mobile" name="mobile" value="" required>
            </div>

            <div class="mb-3 w-50">
                <label class="font-weight-bold" for="housename">House Name *</label>
                <input type="text" class="form-control " id="housename" name="housename" value="" required>
            </div>

            <div class="row">
                <div class="col-md-4 mb-3">
                    <label class="font-weight-bold" for="city">City *</label>
                    <input type="text" class="form-control " id="city" name="city" value="" required>
                </div><!-- End .col-md-6 -->

                <div class="col-md-4 mb-3">
                    <label class="font-weight-bold" for="state">District *</label>
                       <input type="text" class="form-control " id="district" name="district" value="" required>
                   </div><!-- End .col-md-6 -->

                <div class="col-md-4 mb-3">
                 <label class="font-weight-bold" for="state">State *</label>
                    <input type="text" class="form-control " id="state" name="state" value="" required>
                </div><!-- End .col-md-6 -->
            </div><!-- End .row -->

            <div class="mb-3">
                <label class="font-weight-bold" for="pin">Pin *</label>
                <input type="text" class="form-control w-25 " id="pin" name="pin" value="" required>
            </div>

            <button type="submit" class="btn btn-primary " style="background-color:#717fe0;;">ADD +</button>
        </form>

    </div>
</div>


<%- include('../userMainlayouts/mainfooter.ejs') %>